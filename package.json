{
  "name": "connection-switcher",
  "version": "1.0.0",
  "description": "This software allows you to select a network adapter to enable or disable it easier and faster than going through Windows settings.",
  "main": "src/main/main.js",
  "scripts": {
    "test": "electron .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "build": {
    "win": {
      "requestedExecutionLevel": "highestAvailable"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NzoSifou/connection-switcher.git"
  },
  "keywords": [
    "connection",
    "switch",
    "toggle",
    "ethernet",
    "network",
    "adapter",
    "wifi"
  ],
  "author": "NzoSifou",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/NzoSifou/connection-switcher/issues"
  },
  "homepage": "https://github.com/NzoSifou/connection-switcher#readme",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@types/electron": "^1.6.10",
    "electron": "^22.0.0",
    "electron-store": "latest",
    "sudo-prompt": "latest",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "latest",
    "sudo-prompt": "latest"
  }
}
